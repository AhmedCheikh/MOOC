<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Mooc\MoocBundle\Entity\Organisme" table="organisme">
    <indexes>
      <index name="fk_org" columns="iduser"/>
    </indexes>
    <id name="idorganisme" type="integer" column="idorganisme">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="true"/>
    <field name="email" type="string" column="email" length="45" nullable="true"/>
    <field name="siteweb" type="string" column="siteweb" length="45" nullable="true"/>
    <field name="adresse" type="string" column="adresse" length="100" nullable="true"/>
    <field name="telephone" type="string" column="telephone" length="10" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="document" type="blob" column="document" nullable="true"/>
    <field name="logo" type="blob" column="logo" nullable="true"/>
    <many-to-one field="iduser" target-entity="Utilisateur">
      <join-columns>
        <join-column name="iduser" referenced-column-name="idUser"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
