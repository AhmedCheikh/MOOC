<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Mooc\MoocBundle\Entity\Utilisateur" table="utilisateur">
    <unique-constraints>
      <unique-constraint name="idUser" columns="idUser"/>
    </unique-constraints>
    <id name="iduser" type="integer" column="idUser">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="login" length="45" nullable="false"/>
    <field name="password" type="string" column="password" length="45" nullable="false"/>
    <field name="role" type="string" column="role" length="45" nullable="false"/>
  </entity>
</doctrine-mapping>
