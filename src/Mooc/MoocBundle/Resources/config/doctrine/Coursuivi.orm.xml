<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Mooc\MoocBundle\Entity\Coursuivi" table="coursuivi">
    <indexes>
      <index name="fk_app1" columns="cinapprenant"/>
      <index name="fk_cours" columns="id_cours"/>
    </indexes>
    <id name="idcoursuivi" type="integer" column="idCoursuivi">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="commentaire" type="string" column="commentaire" length="255" nullable="false"/>
    <field name="note" type="float" column="note" precision="10" scale="0" nullable="false"/>
    <field name="dateDebut" type="date" column="date_debut" nullable="false"/>
    <field name="appreciation" type="string" column="appreciation" length="255" nullable="false"/>
    <many-to-one field="cinapprenant" target-entity="Apprenant">
      <join-columns>
        <join-column name="cinapprenant" referenced-column-name="cin"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCours" target-entity="Cours">
      <join-columns>
        <join-column name="id_cours" referenced-column-name="idcours"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
